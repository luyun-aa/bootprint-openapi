{{!--
    Renders the response definitions
--}}
{{#if responses}}
<div style="display:none">
  <h2>Response definitions</h2>
  <dl id="sw-response-definitions">
      {{#each responses}}
          <dt><a name="{{key}}"></a>{{@key}}</dt>
          <dd>
              {{> swagger/response response=.}}
          </dd>
      {{/each}}
  </dl>
</div>
{{/if}}
